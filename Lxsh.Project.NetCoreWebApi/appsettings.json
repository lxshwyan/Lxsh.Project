{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },

  "Middleware": {
    "RequestResponseLog": {
      "Enabled": false
    },
    "IPLog": {
      "Enabled": true
    },
    "RecordAccessLogs": {
      "Enabled": true
    },
    "ExpirationTime": { //api过期时间
      "Time": ""
    }
  },
  "AllowedHosts": "*"
}
